					<xsd:documentation>
						<Description>Routing Transit Number</Description>
						<LineNumber>11b</LineNumber>
						<ELFFieldNumber>1272</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!--Bank Account Type Code -->
			<xsd:element name="BankAccountTypeCd" type="BankAccountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Bank Account Type Code</Description>
						<LineNumber>11c</LineNumber>
						<ELFFieldNumber>1274 1276</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!--Depositor Account Number -->
			<xsd:element name="DepositorAccountNumber" type="BankAccountNumberType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Depositor Account Number</Description>
						<LineNumber>11d</LineNumber>
						<ELFFieldNumber>1278</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- =========== Amount You Owe =========== -->
