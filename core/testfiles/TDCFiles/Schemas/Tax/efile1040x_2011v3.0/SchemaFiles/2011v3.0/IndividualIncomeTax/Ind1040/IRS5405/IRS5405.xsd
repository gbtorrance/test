			<!--Part II - Credit-->
			<!--Home Purchase Price Amount -->
			<!--
			<xsd:element name="HomePurchasePriceAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Home Purchase Price Amount</Description>
