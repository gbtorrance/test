		<ParentNodeStyle>
			<FontName>Calibri</FontName>
			<FontHeight>11</FontHeight>
			<ColorRGB>0 0 255</ColorRGB> <!-- blue -->
			<Italic>false</Italic>
		</ParentNodeStyle>
		<AttribNodeStyle>
			<FontName>Calibri</FontName>
			<FontHeight>11</FontHeight>
			<ColorRGB>34 97 13</ColorRGB> <!-- green -->
		</AttribNodeStyle>
		<CompositorNodeStyle>
			<FontName>Calibri</FontName>
			<FontHeight>11</FontHeight>
			<ColorRGB>64 64 64</ColorRGB> <!-- dark gray -->
			<Italic>true</Italic>
		</CompositorNodeStyle>
