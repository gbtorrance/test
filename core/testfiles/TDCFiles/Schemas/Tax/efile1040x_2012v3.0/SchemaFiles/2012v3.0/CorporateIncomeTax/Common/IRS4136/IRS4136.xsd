							<xsd:attribute name="creditReferenceNumber" type="StringType" fixed="324" use="required" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Exported Nontaxable Aviation Gasoline - Gallons -->
			<xsd:element name="ExpNontaxableAviationGasGals" type="FuelGallonsType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Exported nontaxable aviation gasoline - gallons</Description>
						<LineNumber>2c(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Exported Nontaxable Aviation Gasoline - Credit -->
			<xsd:element name="ExpNontaxableAviationGasCredit" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Exported nontaxable aviation gasoline - credit</Description>
						<LineNumber>2c(d)</LineNumber>
					</xsd:documentation>
