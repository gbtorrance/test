						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Bus Use of Undyed Diesel - Gallons -->
			<xsd:element name="UseOfUndyedDieselBusesGallons" type="FuelGallonsType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Use of undyed diesel fuel in buses</Description>
						<LineNumber>3d(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Bus Use of Undyed Diesel - Credit -->
			<xsd:element name="UseOfUndyedDieselInBusesCredit" minOccurs="0">
