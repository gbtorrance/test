					<xsd:documentation>
						<Description>Nontaxable use of undyed kerosene</Description>
						<LineNumber>4a(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Use on a Farm for Farming purposes - Gallons -->
			<xsd:element name="UndyedKeroseneForFarmingGal" type="FuelGallonsType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Use on a farm for farming purposes - Gallons</Description>
						<LineNumber>4b(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
