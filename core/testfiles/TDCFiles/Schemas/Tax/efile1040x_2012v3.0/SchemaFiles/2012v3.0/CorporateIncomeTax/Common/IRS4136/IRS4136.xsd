					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType" />
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="EvidenceOfDyedDieselFuelStatement" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Nontaxable Use of Undyed Diesel Fuel -->
			<xsd:element name="NontaxableUseOfUndyedDiesel" type="UndyedDieselKeroseneType" minOccurs="0" maxOccurs="8">
