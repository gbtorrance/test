					<xsd:documentation>
						<Description>Use of undyed diesel fuel in trains</Description>
						<LineNumber>3c(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Train Use of Undyed Diesel - Credit -->
			<xsd:element name="TrainUseOfUndyedDieselCredit" minOccurs="0">
