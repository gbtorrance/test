						<Description>Exported undyed diesel fuel - Gallons</Description>
						<LineNumber>3e(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Exported Undyed Diesel Fuel - Credit -->
			<xsd:element name="ExportedUndyedDieselFuelCr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Exported undyed diesel fuel - Credit</Description>
						<LineNumber>3e(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountNNType">
							<xsd:attribute name="creditReferenceNumber" type="StringType" fixed="413" use="required" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Exception to Nontaxable Use of Undyed Kerosene -->
			<xsd:element name="UndyedKeroseneUseException" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Kerosene contains visible evidence of dye</Description>
						<LineNumber>4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType" />
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="EvidenceOfDyedKeroseneStatement" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Nontaxable Use of Undyed Kerosene -->
			<xsd:element name="NontaxableUseOfUndyedKerosene" type="UndyedDieselKeroseneType" minOccurs="0" maxOccurs="8">
