						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Exported Undyed Diesel Fuel - Gallons -->
			<xsd:element name="ExportedUndyedDieselFuelGals" type="FuelGallonsType" minOccurs="0">
