					<xsd:documentation>
						<Description>Use on farm undyed diesel fuel/nontaxable - credit</Description>
						<LineNumber>3b(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountNNType">
							<xsd:attribute name="creditReferenceNumber" type="StringType" fixed="360" use="required" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Train Use of Undyed Diesel - Gallons -->
