<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.irs.gov/efile" xmlns="http://www.irs.gov/efile"
xmlns:xsd="http://www.w3.org/2001/XMLSchema"
elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">

	<xsd:annotation>
		<xsd:documentation>
			<Description>IRS e-file Schema - Form 4136</Description>
			<TaxYear>2012</TaxYear>
			<MaturityLevel>Final Release</MaturityLevel>
			<ReleaseDate>September 7, 2012</ReleaseDate>
		</xsd:documentation>
	</xsd:annotation>

	<xsd:include schemaLocation="../../../Common/efileTypes.xsd" />


	<!-- ============================================================ -->
	<!-- =======================  IRS Form 4136 ===================== -->
	<!-- ============================================================ -->


	<xsd:element name="IRS4136">
		<xsd:annotation>
			<xsd:documentation>IRS Form 4136</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS4136Type">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute name="documentName" type="xsd:string" fixed="IRS4136">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>

					<xsd:attribute name="referenceDocumentId" type="IdListType" />
					<xsd:attribute name="referenceDocumentName" type="StringType" fixed="BinaryAttachment GeneralDependencySmall" />

				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="IRS4136Type">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 4136</xsd:documentation>
		</xsd:annotation>

		<xsd:sequence>


			<!-- ===== Line 1: Nontaxable Use of Gasoline and Gasohol ===== -->


			<!-- Off-Highway Business Use of Gasoline - Gallons -->
			<xsd:element name="OffHwyBusinessGasolineGallons" type="FuelGallonsType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Off-highway business use of gasoline in gallons</Description>
						<LineNumber>1a(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Use of Gasoline on a Farm for Farming Purposes - Gallons -->
			<xsd:element name="FarmingPurposesGasolineGallons" type="FuelGallonsType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Use of gasoline on a farm for farming purposes</Description>
						<LineNumber>1b(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Nontaxable Use of Gasoline -->
			<xsd:element name="OtherNontaxableUseOfGasoline" type="GasoholType" minOccurs="0" maxOccurs="7">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other nontaxable use of gasoline</Description>
						<LineNumber>1c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Nontaxable Use of Gasoline - Credit -->
			<xsd:element name="NontaxableUseOfGasolineCredit" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Nontaxable use of gasoline credit</Description>
						<LineNumber>1c(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountNNType">
							<xsd:attribute name="creditReferenceNumber" type="StringType" fixed="362" use="required" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!--Exported Noxtaxable Use of Gasoline - Gallons -->
			<xsd:element name="ExportedNontaxableUseOfGasGals" type="FuelGallonsType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Exported noxtaxable use of gasoline - gallons</Description>
						<LineNumber>1d(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Exported Noxtaxable Use of Gasoline - Credit -->
			<xsd:element name="ExportedNontaxableUseOfGasCr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Exported noxtaxable use of gasoline - credit</Description>
						<LineNumber>1d(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountNNType">
							<xsd:attribute name="creditReferenceNumber" type="StringType" fixed="411" use="required" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Commercial Aviation Use of Gasoline - Gallons -->
			<xsd:element name="AviationGasolineGallons" type="FuelGallonsType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Gasoline use in commercial aviation (other than foreign trade)</Description>
						<LineNumber>2a(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Commercial Aviation Use of Gasoline - Credit -->
			<xsd:element name="AviationGasolineCredit" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Amount of credit for gasoline use in commercial aviation (other than foreign trade)</Description>
						<LineNumber>2a(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountNNType">
							<xsd:attribute name="creditReferenceNumber" type="StringType" fixed="354" use="required" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Other Nontaxable Use of Aviation Gasoline -->
			<xsd:element name="NontaxableAviationGasoline" type="AviationFuelGasolineType" minOccurs="0" maxOccurs="8">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other nontaxable use of aviation gasoline</Description>
						<LineNumber>2b(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Nontaxable Use of Aviation Gasoline - Credit -->
			<xsd:element name="NontaxableAviationGasolineCr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Amount of credit for other nontaxable use of aviation gasoline</Description>
						<LineNumber>2b(d)</LineNumber>
